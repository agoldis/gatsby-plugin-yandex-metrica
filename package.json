  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "gatsby": "^2.1.27"
